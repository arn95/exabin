    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "lldb",
            "request": "launch",
            "name": "Analyze",
            "program": "${workspaceFolder}/bin/exabin",
            "args": ["-a", "bin/access.dms", "-s"],
            "cwd": "${workspaceFolder}"
        },

        {
            "type": "lldb",
            "request": "launch",
            "name": "Load",
            "program": "${workspaceFolder}/bin/exabin",
            "args": ["-l", "bin/access.dms.exb"],
            "cwd": "${workspaceFolder}"
        }
    ]
}